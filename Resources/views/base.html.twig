<!DOCTYPE html>
<html>
  <head>
    {% block metas %}
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    {% endblock %}
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block favicon %}
      <link rel="shortcut icon" href="{{ asset('bundles/wgwebsite/images/favicon.ico') }}" />
    {% endblock %}
    {% block stylesheets %}
      <link href="{{ asset('bundles/wgwebsite/lib/chosen/chosen.css') }}" rel="stylesheet" type="text/css" />
      <link href="{{ asset('bundles/wgwebsite/themes/sanfran/style.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}
    {% block conditionals %}
    {% endblock %}
    {% block javascripts %}
      <script src="{{ asset('bundles/wgwebsite/js/jquery-1.11.0.min.js') }}"></script>
      <script src="{{ asset('bundles/wgwebsite/lib/chosen/chosen.jquery.js') }}"></script>
      <script src="{{ asset('bundles/wgwebsite/js/jquery-ui.min.js') }}"></script>
      <script src="{{ asset('bundles/wgwebsite/js/jquery.wgajaxform.js') }}"></script>
      <!--[if lt IE 9]>
        <script src="{{ asset('bundles/wgwebsite/lib/IE9.js') }}">IE7_PNG_SUFFIX=":";</script>
      <![endif]-->
      <!--[if IE 7]>
        <script src="{{ asset('bundles/wgwebsite/js/jquery.ba-hashchange.min.js') }}"></script>
      <![endif]-->
      <script src="{{ asset('bundles/wgwebsite/js/main.js') }}"></script>
    {% endblock %}
  </head>
  <body>
    {% block body %}Written Games website{% endblock %}
  </body>
</html>
